---
applications:
  - name: seaweedfs-smoke-test
    memory: 512M
    disk_quota: 512M
    instances: 1
    buildpacks:
      - python_buildpack
    command: python app.py
    health-check-type: http
    health-check-http-endpoint: /health
    routes:
      - route: seaweedfs-smoke-test.<%= p('smoke_tests.app_domain') %>
